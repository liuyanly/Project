<script>
    var content=new Vue({
        el:'#page',
        data:{
            currentLi:4,
            questionType:0,
            questionValue:'',
            xiaType1:0,
            chartDate:0,
            total:50,
        },
        mounted:function () {

        },
        methods:{
            changeLi:function (num) {
                if(num==0){
                    location.href="health_evaluation.html";
                }else if(num==1){
                    location.href="blood_pressure_evaluation.html";
                }else if(num==2){
                    location.href="blood_fat_evaluation.html";
                }else if(num==3){

                }else{
                    location.href="depth_analysis.html";
                }
                this.currentLi=num;
            },
            changeType:function () {
                if(this.questionType==0){
                    this.questionType=1;
                }else{
                    this.questionType=0;
                }
            },
            changeXia1:function(num){
                if(this.xiaType1==0){
                    this.xiaType1=num;
                }else{
                    this.xiaType1=0;
                }
            },
            upFile:function () {

            },
            changeChartDate:function (type) {
                this.chartDate=type;
            },
            viewInfo:function () {
                location.href='depth_analysis_info.html';
            }
        }
    })
</script>
