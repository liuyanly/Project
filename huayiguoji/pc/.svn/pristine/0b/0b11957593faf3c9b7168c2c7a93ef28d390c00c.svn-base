@import "variable";
.weekday-list{
  margin-left: 15px;
  li{
    width: 105px;
    border-right:1px solid #e5e5e5;
    border-top:1px solid #e5e5e5;
    border-bottom:1px solid #e5e5e5;
    line-height: 28px;
    height: 27px;
    text-align: center;
    .color999(14px,#000);
    float: left;
    box-sizing: border-box;
    &:nth-of-type(1){
      border-bottom: none;
      border-left: 1px solid #e5e5e5;
    }
  }

}
.amPm{
  margin-left: 15px;
  li{
    width: 105px;
    border-left:1px solid #e5e5e5;
    border-right:1px solid #e5e5e5;
    border-top:1px solid #e5e5e5;
    line-height: 27px;
    height: 28px;
    text-align: center;
    .color999(14px,#555);
    box-sizing: border-box;
    &:last-child{
      border-bottom: 1px solid #e5e5e5;
      height: 29px;
    }
    &:nth-of-type(3n-2){
      background: #F9F6F6;
    }
  }
  .firstLi{
    height: 27px;
  }
}
.date-list{
  width: 740px;
  .date-ul{
    float: left;
    li{
      width: 105px;
      border-right:1px solid #e5e5e5;
      border-bottom:1px solid #e5e5e5;
      height: 28px;
      text-align: center;
      box-sizing: border-box;
      .color999(14px,#666);
      display: -webkit-box;
      display: -webkit-flex;
      display: -ms-flexbox;
      display: flex;
      -ms-flex-wrap:wrap;
      flex-wrap:wrap;
      -webkit-box-pack: center;
      -webkit-justify-content: center;
      -ms-flex-pack: center;
      justify-content: center;
      -webkit-box-align: center;
      -webkit-align-items: center;
      -ms-flex-align: center;
      align-items: center;
      span{
        margin-right: 3px;
      }
      &:nth-of-type(1){
        background: #F9F6F6;
      }
    }
    .date{
      color:#555;
    }
    .bg{
      background: #4CC6D8;
      color: #fff;
    }
  }
}