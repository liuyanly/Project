.cc {
  font-size: 14px;
  color: #4CC6D8;
}
.header-title {
  line-height: 50px;
  border-bottom: 1px solid #eff1fa;
  padding: 0 20px;
  font-size: 14px;
  color: #333;
}
.main .container .right-box {
  height: auto;
  margin-bottom: 30px;
}
.description-content {
  padding: 15px 15px;
  margin-bottom: 22px;
}
.description-content .description-word p {
  line-height: 30px;
  height: 30px;
}
.description-content .description-word .title-name {
  font-size: 14px;
  color: #000;
}
.description-content .description-word .content-word {
  font-size: 14px;
  color: #999;
}
.description-table {
  width: 848px;
  margin: 0 auto;
}
.description-table tr,
.description-table td {
  border: 1px solid #e5e5e5;
  text-align: center;
  font-size: 14px;
  color: #555;
  line-height: 28px;
  height: 29px;
}
.description-table tr:nth-of-type(2n-1) {
  background: #f9f6f6;
}
.description-table .imp td {
  color: #EE784E;
}
.description-table .imp td .abnormal {
  display: inline-block;
  width: 30px;
}
.description-table thead .th-tr {
  background: #fff;
}
.description-table thead td {
  font-size: 14px;
  color: #000;
  line-height: 28px;
}
.description-table .w1,
.description-table .w2,
.description-table .w3,
.description-table .w4 {
  font-size: 14px;
  color: #000;
  line-height: 28px;
}
.description-table .w1 {
  width: 134px;
}
.description-table .w2 {
  width: 229px;
}
.description-table .w3 {
  width: 227px;
}
.description-table .w4 {
  width: 254px;
}
.data-table td {
  padding-left: 10px;
  text-align: left;
}
.data-table .chart-btn {
  cursor: pointer;
}
.data-list {
  padding: 14px 15px;
  margin-bottom: 16px;
}
.data-list li {
  float: left;
  padding: 0 11px;
  border-radius: 3px;
  line-height: 31px;
  height: 31px;
  font-size: 14px;
  color: #555;
  background: #EFF1FA;
  margin-bottom: 16px;
  margin-right: 10px;
  cursor: pointer;
}
.data-list li.current {
  background: #4cc6d8;
  color: #fff;
}
.data-list li.current .data {
  background: #fff;
  color: #4CC6D8;
}
.data-list li .data {
  width: 20px;
  font-size: 12px;
  color: #fff;
  line-height: 20px;
  height: 20px;
  text-align: center;
  border-radius: 50%;
  background: #EE774D;
  margin-left: 4px;
  margin-top: 6px;
}
.pic-list {
  padding: 11px 15px;
  margin-bottom: 46px;
}
.pic-list li {
  float: left;
  width: 132px;
  height: 180px;
  background: #f3f7fd;
  margin-right: 11px;
  margin-bottom: 10px;
  cursor: pointer;
}
.pic-list li:nth-of-type(6) {
  margin-right: 0;
}
.pic-list li img {
  width: 132px;
  height: 180px;
}
.fade {
  width: 100%;
  height: 100vh;
  overflow: auto;
  background: rgba(0, 0, 0, 0.2);
  text-align: center;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 1000;
}
.fade .close-box {
  width: 100%;
  height: 100vh;
  overflow: auto;
  background: rgba(0, 0, 0, 0);
  text-align: center;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 1000;
}
.img-container {
  position: fixed;
  top: 0px;
  right: 0px;
  left: 0px;
  bottom: 0px;
  text-align: center;
  vertical-align: middle;
  display: -webkit-box;
  display: flex;
  display: -webkit-flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  -ms-flex-align: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
  -ms-flex-pack: center;
  justify-content: center;
  align-items: center;
}
.img-container .fade {
  z-index: 999;
  position: absolute;
}
.img-container .img-box {
  text-align: center;
  overflow: auto;
  background: rgba(0, 0, 0, 0);
  z-index: 1000;
  position: relative;
}
.img-container .img-box .img {
  border: 11px solid #fff;
  max-height: 100vh;
}
.img-container .img-box .pic-p {
  font-size: 50px;
  position: absolute;
  top: -10px;
  right: 0;
  color: white;
  cursor: pointer;
}
.img-container .img-box .left-img {
  margin-right: 27px;
  cursor: pointer;
}
.img-container .img-box .right-img {
  margin-left: 27px;
  cursor: pointer;
}
