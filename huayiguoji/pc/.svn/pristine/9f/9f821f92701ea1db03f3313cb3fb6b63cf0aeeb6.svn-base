        position: absolute;
        right: 145px;
        height: 20px;

        .name {
            width: 20px;
            font-size: 12px;
            color: #fff;
            text-align: center;
            height: 16px;
            line-height: 16px;
            background-color: #00b3ee;
            margin-top: 2px;
        }

