@import "variable";

abbr,
address,
article,
aside,
audio,
b,
blockquote,
body,
canvas,
caption,
cite,
code,
dd,
del,
details,
dfn,
div,
dl,
dt,
em,
fieldset,
figcaption,
figure,
footer,
form,
h1,
h2,
h3,
h4,
h5,
h6,
header,
hgroup,
html,
i,
iframe,
img,
ins,
kbd,
label,
legend,
li,
mark,
menu,
nav,
object,
ol,
p,
pre,
q,
samp,
section,
small,
strong,
sub,
summary,
sup,
table,
tbody,
td,
tfoot,
th,
thead,
time,
tr,
ul,
var,
video {
    font-size: 14px;
}

.content {
    width: auto;
}

body {
    background: #fff;
}

.main {
    line-height: 1;
    padding: 0;
}

.main-box {
    background: #F3F7FD;

    .container {
        background: #fff;
    }
}

.footer {
    height: auto;
}

button,
input,
select,
textarea {
    font-family: "Microsoft YaHei";
    font-size: 14px;
}

input {
    color: #333;
    font-size: 14px;
}

input::-webkit-input-placeholder,
textarea::-webkit-input-placeholder {
    /* WebKit browsers */
    color: #999;
    font-size: 14px;
}

input:-moz-placeholder,
textarea:-moz-placeholder {
    /* Mozilla Firefox 4 to 18 */
    color: #999;
    font-size: 14px;
}

input::-moz-placeholder {
    /* Mozilla Firefox 19+ */
    color: #999;
    font-size: 14px;
}

input:-ms-input-placeholder,
textarea:-ms-input-placeholder {
    /* Internet Explorer 10+ */
    color: #999;
    font-size: 14px;
}

.container {
    width: 1000px;
    margin: 0 auto;
}

.cursor {
    cursor: pointer;
}

select {
    appearance: none;
    -moz-appearance: none;
    -webkit-appearance: none;
}

select::-ms-expand {
    display: none;
}

.main p {
    margin-bottom: 0;
}

.swiper-container {
    width: 100%;
    height: 422px;

    .swiper-wrapper {
        width: 100%;
        height: 100%;
    }

    .swiper-slide {
        background-position: center;
        background-size: cover;
        width: 100%;
        height: 100%;

        img {
            width: 100%;
            height: 100%;
        }
    }

    .swiper-pagination-bullet {
        width: 0.833rem;
        height: 0.833rem;
        display: inline-block;
    }
}

.swiper-pagination-white .swiper-pagination-bullet-active {
    background: #1cb6cd;
}

.swiper-container-horizontal > .swiper-pagination-bullets .swiper-pagination-bullet {
    margin: 0 9px 0 0;
}

.swiper-container .swiper-pagination-bullet {
    width: 6px;
    height: 6px;
    display: inline-block;
}

.swiper-pagination-bullet {
    opacity: 1;
}

.swiper-container-horizontal > .swiper-pagination-bullets,
.swiper-pagination-custom,
.swiper-pagination-fraction {
    bottom: 60px;
    left: 0;
    width: 100%;
}

.login-area {
    margin-top: 13px;

    .login-p {
        margin-right: 37px;
        line-height: 12px;
    }

    a,
    span {
        width: 44px;
        font-size: 12px;
        text-align: center;
        display: inline-block;
        cursor: pointer;
        color: #666;
        line-height: 12px;
    }

    a:nth-of-type(1) {
        border-right: 1px solid #666;
    }

    .already-login {
        color: #4cc6d8;
        padding-right: 10px;
        width: auto;
        border-right: 1px solid #666;
    }

    .msg-center {
        font-size: 12px;
        color: #666;
        width: auto;
        margin-top: 1px;
        line-height: 12px;
    }

    .c1 {
        color: #4CC6D8;
        line-height: 12px;
        font-size: 12px;
    }

    .upload {
        width: 80px;
    }

    .login-div {
        margin-right: 0;

        span {
            width: auto!important;
            border: none!important;
        }
    }
}

.nav {
    height: 90px;

    .logo-box {
        height: 90px;
        line-height: 90px;

        img {
            width: 40px;
            height: 30px;
            margin: 30px 6px 30px 0;
            vertical-align: middle;
            float: left;
        }

        .logo-name {
            width: 117px;
            //height:26px;
            //background:rgba(46,54,142,1);
            color: #2e368e;
            font-size: 26px;
            //margin-top: 24px;
