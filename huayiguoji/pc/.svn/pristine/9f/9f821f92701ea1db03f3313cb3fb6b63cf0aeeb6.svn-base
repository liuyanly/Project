.chart-box {
    position: relative;
    height: 1000px;
}

.Validform_error {
    background-color: #ffe7e7 !important;
}
//左边导航样式

.left-nav {
    width: 116px;
    float: left;
    height: 720px;
    background: #4CC6D8;
    border-radius: 3px;

    .outer-list {
        border-radius: 3px;

        & > li {
            width: 100%;
            text-align: center;
            line-height: 39px;
            color: #fff;
            background: #4CC6D8;
            border-bottom: 1px solid #17B5CC;
            cursor: pointer;
            font-size: 14px;

            &:nth-of-type(1) {
                border-top-left-radius: 3px;
                border-top-right-radius: 3px;
            }

            p:hover {
                background: #2748AB;
                border-bottom: none;
            }

            p {
                padding: 0 30px;
                text-align: left;
                position: relative;

                &:nth-of-type(1) {
                    border-top-left-radius: 3px;
                    border-top-right-radius: 3px;
                }
            }

            p > img {
                position: absolute;
                left: 93px;
                top: 16px;
            }

            .active {
                background: #2748AB;
                border-bottom: 1px solid #2748ab;

                p {
                    background: #2748AB;
                }
            }

            .secMenu {
                .sonActive {
                    background: #2748AB;
                    border-bottom: 1px solid #2748ab;
                }

                li {
                    padding: 0 30px;
                    text-align: left;
                    background: #13B0C7;
                    border-bottom: 1px solid #17B5CC;
                    font-size: 14px;

                    &:hover {
                        background: #2748AB;
                    }
                }

                li:last-child {
                    border-bottom: none;
                }
            }
        }
        /*li:hover{
      background: #2748AB;
      border-bottom: none;
    }*/
    }
}
