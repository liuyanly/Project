}

.nav-list {
    margin-top: 33px;

    li {
        float: left;
        font-size: 14px;
        width: 70px;
        text-align: center;
        font-family: MicrosoftYaHei-Bold;
        font-weight: 600;
        line-height: 24px;
        height: 24px;
        margin-right: 21px;
        cursor: pointer;
        color: #666;
    }

    li:last-child {
        margin-right: 0;
    }

    li:hover {
