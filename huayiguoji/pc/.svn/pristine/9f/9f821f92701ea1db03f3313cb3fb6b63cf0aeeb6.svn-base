//分页

.page {
    text-align: center;
}

.el-pagination .btn-next .el-icon,
.el-pagination .btn-prev .el-icon {
    display: block;
    font-size: 16px;
    font-weight: 700;
    color: #666;
}

.el-pagination {
    white-space: nowrap;
    padding: 2px 5px;
    color: #2d2f33;
    font-weight: 500;
    text-align: center;
}

.el-pagination button,
.el-pagination span:not([class*=suffix]) {
    font-size: 16px;
    height: 35px;
    line-height: 35px;
}

.el-pager li {
    padding: 0;
    vertical-align: top;
    font-size: 14px;
    color: #666;
    width: 37px;
    height: 35px;
    line-height: 35px;
    box-sizing: border-box;
    text-align: center;
    border-radius: 3px;
}

.el-pager li:hover {
    color: #4CC6D8;
}

.el-pager li.active {
    color: #fff;
    background-color: #4CC6D8;
    cursor: default;
}

.date-select {
    select {
        width: 114px;
        height: 30px;
        line-height: 30px;
        border: 1px solid #eff1fa;
        border-radius: 3px;
        padding-left: 10px;
        //opacity: 0;
        appearance: none;
        -moz-appearance: none;
        -webkit-appearance: none;
        .color999(14px,#999);
        outline-width: 1px;
        background: #FAFBFF;

        option {
            .color999(14px,#999);
            padding: 20px;
            width: 114px;
            text-align: center;
            height: 34px;
            display: inline-block;
            line-height: 34px;
            height: 34px;
            border-bottom: none;
        }

        .placeholder {
            display: none;
            .color999(14px,#999);
        }
    }

    select::-ms-expand {
        display: none;
    }

    select:focus {
        border-color: #409EFF;
    }

    select:hover {
        border-color: #b4bccc;
    }
}

.pos {
    position: relative;
    width: 114px;
    height: 30px;
    margin-right: 9px;

    &:last-child {
        margin-right: 0;
    }

    .xiaLa {
        position: absolute;
        right: 6px;
        top: 11px;
    }
}

.el-rate__icon {
    font-size: 20px;
    color: #6FC3D5 !important;
}

.el-icon-star-on {
