.cc {
  font-size: 14px;
  color: #4CC6D8;
}
.header-title {
  line-height: 50px;
  border-bottom: 1px solid #eff1fa;
  padding: 0 20px;
  font-size: 14px;
  color: #333;
}
.vip-box > li {
  padding: 23px 14px;
  border-bottom: 1px solid #EFF1FA;
}
.vip-box > li .img-p {
  width: 204px;
  height: 93px;
  border-radius: 6px;
  background-repeat: no-repeat;
  background-size: contain;
  text-align: center;
  line-height: 93px;
  color: #fff;
  font-size: 16px;
  font-weight: 600;
}
.vip-box > li .v1 {
  background: url("../images/f69_v1.png");
}
.vip-box > li .v2 {
  background: url("../images/f69_v2.png");
}
.vip-box > li .v3 {
  background: url("../images/f69_v3.png");
}
.vip-box > li .v4 {
  background: url("../images/f69_v4.png");
}
.vip-box > li .c1,
.vip-box > li .c2 {
  margin-left: 13px;
  height: 89px;
  padding: 2px 0;
  display: flex;
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  -webkit-flex-direction: column;
  -ms-flex-direction: column;
  flex-direction: column;
  flex-wrap: wrap;
  -webkit-flex-wrap: wrap;
  -ms-flex-wrap: wrap;
  -webkit-align-items: center;
  -ms-flex-align: center;
  align-items: center ;
}
.vip-box > li .c1 li,
.vip-box > li .c2 li {
  line-height: 22px;
  width: 174px;
  font-size: 14px;
}
.vip-box > li .c1 {
  color: #883F03;
}
.vip-box > li .c2 {
  color: #454545;
}
.vip-box .money {
  font-size: 18px;
  color: #4cc6d8;
  margin-right: 18px ;
  text-align: center;
  line-height: 93px;
  font-weight: 600;
}
.vip-box .open-btn {
  width: 108px;
  height: 30px;
  background: #4cc6d8;
  border-radius: 3px ;
  color: #fff;
  font-size: 14px;
  text-align: center;
  line-height: 30px;
  margin-top: 29px;
  margin-right: 18px;
  cursor: pointer;
}
.name {
  width: 65px;
  margin-right: 16px;
  float: left;
  text-align: left;
  font-size: 14px;
  color: #999;
  line-height: 30px;
}
.select-p {
  position: relative;
  margin-bottom: 7px!important;
  margin-left: 16px;
  margin-top: 30px;
  font-size: 14px;
  color: #333;
}
.select-p > img {
  position: absolute;
  left: 298px;
  top: 12px;
}
.select-p select {
  width: 236px;
  height: 30px;
  background: #f9f9f9;
  border-radius: 3px ;
  border: 1px solid #eee;
  padding: 0 20px;
  line-height: 30px;
  outline: none;
  box-sizing: border-box;
  font-size: 14px;
  color: #333;
}
