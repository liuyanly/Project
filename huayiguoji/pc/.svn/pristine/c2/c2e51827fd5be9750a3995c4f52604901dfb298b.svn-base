.cc {
  font-size: 14px;
  color: #4CC6D8;
}
.header-title {
  line-height: 50px;
  border-bottom: 1px solid #eff1fa;
  padding: 0 20px;
  font-size: 14px;
  color: #333;
}
.sum-money {
  padding: 17px 14px;
  border-bottom: 1px solid #eff1fa;
}
.sum-money .money {
  font-size: 18px;
  color: #ee774d;
  font-weight: 600;
  line-height: 20px;
}
.sum-money .name {
  font-size: 14px;
  color: #666;
  line-height: 15px;
  margin-top: 3px;
  margin-left: 5px;
}
.tip {
  padding: 15px 15px;
  line-height: 15px;
  font-size: 14px;
  color: #999;
}
.agree-p {
  padding: 0 14px;
}
.agree-p input[type="checkbox"] {
  position: absolute;
  left: 0;
  top: 3px;
  width: 18px;
  height: 18px;
  opacity: 0;
}
.agree-p label {
  font-size: 14px;
  color: #666;
}
.agree-p label span {
  font-size: 14px;
  color: #2748AB;
  cursor: pointer;
}
.btn-p .marginLeft {
  margin-bottom: 64px;
}
.fade2 {
  position: fixed;
  top: 0;
  right: 0;
  text-align: center;
  vertical-align: middle;
  display: flex;
  width: 100%;
  height: 100vh;
  z-index: 1001;
}
.fade2 .msg-box {
  width: 100%;
  height: 100vh;
  overflow: auto;
  background: rgba(0, 0, 0, 0.3);
  z-index: 1002;
  position: absolute;
  display: -webkit-box;
  display: flex;
  display: -webkit-flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  -ms-flex-align: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
  -ms-flex-pack: center;
  justify-content: center;
  align-items: center;
}
.fade2 .msg-box .detail-box {
  width: 804px;
  height: 542px;
  background: #fff;
  border-radius: 10px;
  box-sizing: border-box;
  padding: 22px 0;
  position: relative;
}
.fade2 .msg-box .detail-box .title {
  font-size: 18px;
  color: #000;
  font-family: PingFangSC-Medium;
  text-align: center;
  margin: 32px 0 50px 0;
  line-height: 19px;
}
.fade2 .msg-box .detail-box .close-btn {
  font-size: 36px;
  color: #ccc;
  position: absolute;
  top: 2px;
  cursor: pointer;
  right: 24px;
}
.fade2 .msg-box .detail-box .content-word {
  font-family: "PingFang SC";
  width: 556px;
  padding-right: 88px;
  height: 328px;
  margin-left: 127px;
  overflow: auto;
  font-size: 16px;
}
.fade2 .msg-box .detail-box .btn-p {
  margin-top: 30px;
  text-align: center;
}
.fade2 .msg-box .detail-box .btn-p button {
  width: 136px;
  height: 40px;
  background: #4cc6d8;
  border-radius: 3px ;
  cursor: pointer;
  font-size: 14px;
  color: #fff;
  border: none;
}
.content-word::-webkit-scrollbar-track {
  width: 6px;
  border-radius: 3px;
  background-color: #F1F1F1;
}
.content-word::-webkit-scrollbar {
  width: 6px;
  background-color: #F1F1F1;
}
.content-word::-webkit-scrollbar-thumb {
  border-radius: 3px;
  background-color: #2748AB;
}
