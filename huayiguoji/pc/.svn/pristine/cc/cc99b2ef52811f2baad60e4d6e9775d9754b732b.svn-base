.cc {
  font-size: 14px;
  color: #4CC6D8;
}
.header-title {
  line-height: 50px;
  border-bottom: 1px solid #eff1fa;
  padding: 0 20px;
  font-size: 14px;
  color: #333;
}
.main {
  background: #F3F7FD;
}
.main .container {
  padding-top: 15px;
  padding-bottom: 49px;
}
.main .container .right-box {
  width: 878px;
  background: #fff;
  height: 720px;
}
.main .container .right-box .header-title {
  padding: 0 14px;
  line-height: 50px;
  font-size: 14px;
  color: #333;
  border-bottom: 1px solid #EFF1FA;
}
.main .container .right-box .header-title .add-btn {
  margin-top: 17px;
  margin-right: 14px;
  cursor: pointer;
}
.main .container .right-box .header-title .right {
  line-height: 50px;
  font-size: 14px;
  color: #4ec7d9;
  margin-right: 24px;
  cursor: pointer;
}
.main .container .right-box .small-title {
  font-size: 14px;
  color: #4ec7d9;
  border-top: 1px solid #eff1fa;
}
.main .container .right-box .user-info {
  padding: 15px 16px;
  background: #eff1fa;
  border-radius: 10px ;
  margin: 14px 14px 17px 10px;
  height: 68px;
}
.main .container .right-box .user-info .left-info-box {
  margin-left: 28px;
  margin-top: 5px;
}
.main .container .right-box .user-info .head-box {
  position: relative;
}
.main .container .right-box .user-info .head-box .head-pic {
  width: 70px;
  height: 70px;
  border-radius: 50%;
}
.main .container .right-box .user-info .head-box .vip {
  position: absolute;
  bottom: 2px;
  width: 20px;
  right: 0;
}
.main .container .right-box .user-info .user-name {
  font-size: 14px;
  color: #000;
}
.main .container .right-box .user-info .user-name .marginRight {
  margin-right: 20px;
}
.main .container .right-box .user-info .user-name .sex,
.main .container .right-box .user-info .user-name .age {
  font-size: 14px;
  color: #999;
}
.main .container .right-box .user-info .user-sex,
.main .container .right-box .user-info .user-ad {
  font-size: 12px;
  color: #999;
  margin-top: 12px;
  margin-bottom: 10px;
}
.main .container .right-box .user-info .user-sex .marginRight,
.main .container .right-box .user-info .user-ad .marginRight {
  margin-right: 30px;
}
.main .container .right-box .user-info .edit-btn {
  margin: 22px 14px;
  cursor: pointer;
}
.main .container .right-box .user-info .application-btn {
  margin: 20px 14px;
  width: 108px;
  height: 30px;
  background: #4CC6D8;
  border: none;
  border-radius: 3px;
  color: #fff;
  line-height: 30px;
  font-size: 14px;
  text-align: center;
  cursor: pointer;
}
.main .ul-box li {
  height: 38px;
  border-bottom: 1px solid #EFF1FA;
  padding: 10px 15px;
}
.main .ul-box li > img {
  width: 40px;
  height: 40px;
  border-radius: 50%;
}
.main .ul-box li .info-box {
  margin-left: 13px;
}
.main .ul-box li .info-box .name {
  font-size: 14px;
  color: #000;
  margin-bottom: 8px;
}
.main .ul-box li .info-box .sex {
  font-size: 12px;
  color: #999;
}
.main .ul-box li .info-box .sex .marginRight {
  margin-right: 20px;
}
.main .ul-box li .telNum {
  line-height: 34px;
  height: 34px;
  font-size: 12px;
  color: #999;
  margin-left: 170px;
}
.main .ul-box li .del-btn {
  margin-right: 24px;
  margin-top: 12px;
  cursor: pointer;
}
.fade {
  position: fixed;
  top: 0px;
  right: 0px;
  left: 0px;
  bottom: 0px;
  text-align: center;
  vertical-align: middle;
  display: flex;
  width: 100%;
  height: 100vh;
  z-index: 1100;
}
.msg-box {
  width: 100%;
  height: 100vh;
  overflow: auto;
  background: rgba(0, 0, 0, 0.2);
  z-index: 999;
  position: absolute;
  display: -webkit-box;
  display: flex;
  display: -webkit-flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  -ms-flex-align: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
  -ms-flex-pack: center;
  justify-content: center;
  align-items: center;
}
.msg-box .content-box {
  width: 370px;
  height: 226px;
  background: #fff;
  border-radius: 6px;
  box-sizing: border-box;
}
.msg-box .content-box .title {
  border-top-right-radius: 6px;
  border-top-left-radius: 6px;
  padding: 0 14px;
  font-size: 14px;
  text-align: left;
  color: #fff;
  background: #4CC6D8;
  line-height: 40px;
  height: 40px;
}
.msg-box .content-box .title .close-btn {
  font-size: 26px;
  cursor: pointer;
}
.msg-box .content-box .tip {
  padding: 14px;
  text-align: left;
  font-size: 14px;
  color: #999;
  line-height: 30px;
  height: 30px;
}
.msg-box .content-box .input-p {
  padding: 0 40px ;
  margin-top: 54px;
  margin-bottom: 20px;
  font-size: 16px;
  color: #666;
  text-align: center;
}
.msg-box .content-box .input-p input {
  width: 288px;
  height: 38px ;
  border: 1px solid #eff1fa;
  background: #f3f7fd;
  text-align: center;
  line-height: 38px;
  font-size: 16px;
  color: #333;
  border-radius: 3px;
  outline: none;
}
.msg-box .content-box .input-p input::-webkit-input-placeholder {
  /* WebKit browsers */
  color: #333;
}
.msg-box .content-box .input-p input:-moz-placeholder {
  /* Mozilla Firefox 4 to 18 */
  color: #333;
}
.msg-box .content-box .input-p input::-moz-placeholder {
  /* Mozilla Firefox 19+ */
  color: #333;
}
.msg-box .content-box .input-p input:-ms-input-placeholder {
  /* Internet Explorer 10+ */
  color: #333;
}
.msg-box .content-box .btn-p {
  text-align: center;
}
.msg-box .content-box .btn-p button {
  border: none;
  cursor: pointer;
  height: 30px;
  width: 80px;
  line-height: 30px;
  text-align: center;
  background: #4CC6D8;
  color: #fff;
  font-size: 14px;
  border-radius: 3px;
}
.msg-box .content-box .btn-p .cancel {
  margin-right: 50px;
}
