.cc {
  font-size: 14px;
  color: #4CC6D8;
}
.header-title {
  line-height: 50px;
  border-bottom: 1px solid #eff1fa;
  padding: 0 20px;
  font-size: 14px;
  color: #333;
}
.msg-box .msg-list li {
  border-bottom: 1px solid #eff1fa;
  padding: 10px 20px;
  cursor: pointer;
}
.msg-box .msg-list li > img {
  width: 40px;
  height: 40px;
  border-radius: 50%;
  margin-right: 14px;
}
.msg-box .msg-list li .content-word {
  width: 729px;
}
.msg-box .msg-list li .content-word .line1 {
  line-height: 24px;
}
.msg-box .msg-list li .content-word .line1 .left {
  font-size: 14px;
  color: #000;
}
.msg-box .msg-list li .content-word .line1 .left .dep {
  color: #2748AB;
}
.msg-box .msg-list li .content-word .line1 .right {
  font-size: 12px;
  color: #999;
}
.msg-box .msg-list li .content-word .line2 {
  font-size: 12px;
  color: #999;
  margin-top: 3px;
  line-height: 12px;
}
.msg-box .msg-list li .content-word .line2 .marginRight {
  margin-right: 15px;
}
.msg-box .msg-list li .del-btn {
  line-height: 40px;
  margin-right: 4px;
  cursor: pointer;
}
.msg-box .tem-list li .content-word {
  width: 741px;
}
.msg-box .tem-list li .line2 {
  margin-top: 0;
  line-height: 16px;
}
.msg-box .tem-list li .sure-btn {
  width: 60px;
  height: 35px;
  background: #4cc6d8;
  border-radius: 3px ;
  text-align: center;
  font-size: 14px;
  color: #fff;
  line-height: 35px;
  margin-top: 1px;
  margin-right: 10px;
}
.bottom-div .btn-p button {
  width: 136px;
  height: 40px;
  background: #45c6da;
  border-radius: 3px ;
  border: none;
  cursor: pointer;
  text-align: center;
  font-size: 14px;
  color: #fff;
  line-height: 29px;
}
.search-box {
  padding: 10px 20px;
  border-bottom: 1px solid #eff1fa;
}
.search-box .input-p {
  line-height: 40px;
}
.search-box .input-p span {
  font-size: 14px;
  color: #4cc6d8;
  margin-right: 10px;
}
.search-box .input-p input {
  width: 285px;
  height: 30px;
  padding: 0 5px;
  line-height: 30px;
  border: 1px solid #999;
  outline: none;
}
.search-box .input-p input::-webkit-input-placeholder {
  /* WebKit browsers */
  color: #cdcdcd;
  font-size: 12px;
}
.search-box .input-p input:-moz-placeholder {
  /* Mozilla Firefox 4 to 18 */
  color: #cdcdcd;
  font-size: 12px;
}
.search-box .input-p input::-moz-placeholder {
  /* Mozilla Firefox 19+ */
  color: #cdcdcd;
  font-size: 12px;
}
.search-box .input-p input:-ms-input-placeholder {
  /* Internet Explorer 10+ */
  color: #cdcdcd;
  font-size: 12px;
}
.search-box .btn-p button {
  width: 136px;
  height: 40px;
  background: #45c6da;
  border-radius: 3px ;
  text-align: center;
  line-height: 40px;
  font-size: 14px;
  color: #fff;
  border: none;
  cursor: pointer;
}
.send-msg {
  width: 80px;
  height: 30px;
  line-height: 30px;
  margin-top: 10px;
  background: #4CC6D8;
  font-size: 14px;
  color: #fff;
  text-align: center;
  border-radius: 3px;
  cursor: pointer;
}
