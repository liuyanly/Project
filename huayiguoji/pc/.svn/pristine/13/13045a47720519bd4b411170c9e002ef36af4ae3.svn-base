.cc {
  font-size: 14px;
  color: #4CC6D8;
}
.header-title {
  line-height: 50px;
  border-bottom: 1px solid #eff1fa;
  padding: 0 20px;
  font-size: 14px;
  color: #333;
}
.main .container .right-box {
  height: auto;
}
.main .container {
  padding-bottom: 0;
}
.main .container .right-box .header-title .add-btn {
  width: 15px;
  height: 15px;
  margin-right: 8px;
  margin-top: -2px;
}
.question-list {
  /* width: 100%; */
  height: 60px;
  overflow-y: hidden;
  overflow-x: auto;
  white-space: nowrap;
}
.question-list li {
  display: inline-block;
  height: 48px;
  line-height: 48px;
  padding: 0 14px;
  cursor: pointer;
  color: #666;
  margin-right: 22px;
  font-size: 14px;
}
.question-list li.active {
  color: #fff;
  background: #4CC6D8;
}
.small-title .right {
  margin-top: 16px;
  cursor: pointer;
}
.list {
  margin-bottom: 69px;
}
.type-list {
  padding: 11px 15px;
  margin-bottom: 17px;
}
.type-list .type-title {
  line-height: 32px;
  height: 32px;
  font-size: 14px;
  color: #000;
}
.type-list table {
  width: 100%;
}
.type-list table tr,
.type-list table td {
  line-height: 28px;
  border: 1px solid #e5e5e5;
  padding-left: 10px;
  text-align: left;
  font-size: 14px;
  color: #666;
}
.type-list table tr:nth-of-type(2n) {
  background: #F9F6F6;
}
.fade {
  position: fixed;
  top: 0px;
  right: 0px;
  left: 0px;
  bottom: 0px;
  text-align: center;
  vertical-align: middle;
  display: flex;
}
.msg-box {
  width: 100%;
  height: 100vh;
  overflow: auto;
  background: rgba(0, 0, 0, 0.2);
  z-index: 999;
  position: absolute;
  display: -webkit-box;
  display: flex;
  display: -webkit-flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  -ms-flex-align: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
  -ms-flex-pack: center;
  justify-content: center;
  align-items: center;
}
.msg-box .content-box {
  width: 300px;
  height: 220px;
  background: #fff;
  border-radius: 6px;
  box-sizing: border-box;
}
.msg-box .content-box .title {
  border-top-right-radius: 6px;
  border-top-left-radius: 6px;
  padding: 0 14px;
  font-size: 14px;
  text-align: left;
  color: #fff;
  background: #4CC6D8;
  line-height: 40px;
  height: 40px;
}
.msg-box .content-box .title .close-btn {
  font-size: 26px;
  cursor: pointer;
}
.msg-box .content-box .tip {
  padding: 14px;
  text-align: left;
  font-size: 14px;
  color: #999;
  line-height: 30px;
  height: 30px;
}
.msg-box .content-box .select-p {
  text-align: left;
  padding: 0 14px;
  line-height: 30px;
  height: 30px;
  position: relative;
}
.msg-box .content-box .select-p select {
  width: 100%;
  padding: 0 10px;
  border-radius: 3px;
  color: #666;
  border: 1px solid #e5e5e5;
  line-height: 35px;
  height: 35px;
  appearance: none;
  -moz-appearance: none;
  -webkit-appearance: none;
}
.msg-box .content-box .select-p img {
  position: absolute;
  right: 25px;
  top: 15px;
}
.msg-box .content-box .btn-p {
  padding: 14px;
  margin-top: 20px;
  text-align: center;
}
.msg-box .content-box .btn-p button {
  border: none;
  cursor: pointer;
  height: 30px;
  width: 80px;
  line-height: 30px;
  text-align: center;
  background: #4CC6D8;
  color: #fff;
  border-radius: 3px;
}
.msg-box .content-box .btn-p .cancel {
  margin-right: 50px;
}
