.cc {
  font-size: 14px;
  color: #4CC6D8;
}
.header-title {
  line-height: 50px;
  border-bottom: 1px solid #eff1fa;
  padding: 0 20px;
  font-size: 14px;
  color: #333;
}
.patient-list li {
  padding: 10px 20px;
  border-bottom: 1px solid #eff1fa;
}
.patient-list li > img {
  width: 40px;
  height: 40px;
  border-radius: 50%;
  margin-right: 13px;
}
.patient-list li .line1 {
  line-height: 22px;
}
.patient-list li .line1 span {
  font-size: 14px;
  color: #666;
}
.patient-list li .line1 .name {
  color: #4CC6D8;
  margin-right: 18px;
}
.patient-list li .line2 {
  font-size: 12px;
  color: #999;
  margin-top: 6px;
  line-height: 12px;
}
.patient-list li .line2 span {
  float: left;
  line-height: 12px;
}
.patient-list li .line2 .sex {
  margin-right: 18px;
}
.patient-list li > .right span {
  float: left;
  width: 100px;
  height: 40px;
  border-radius: 3px;
  line-height: 40px;
  text-align: center;
  color: #fff;
  font-size: 14px;
  cursor: pointer;
  margin-right: 20px;
}
.patient-list li > .right .send-msg {
  background: #FFA018;
}
.patient-list li > .right .tip {
  background: #69CA56;
}
.patient-list li > .right .view-detail {
  background: #4CC6D8;
  margin-right: 0;
}
.el-pagination {
  text-align: center;
}
