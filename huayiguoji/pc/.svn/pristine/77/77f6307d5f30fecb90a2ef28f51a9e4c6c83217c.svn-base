    <script src="js/vue.js"></script>
    <script src="js/axios.min.js"></script>
    <script src="js/jquery-3.1.0.js"></script>
    <script src="js/Validform_v5.3.2.js"></script>
    <script src="element/element-ui.min.js"></script>
    <script src="swiper/swiper-3.4.2.min.js"></script>
    <script src="js/header.js"></script>
    <script src="js/task_leftNav.js"></script>
    <script src="js/doctor_login.js"></script>
    <script src="js/slider.js"></script>
    <script src="js/footer.js"></script>
    <script src="js/public.js"></script>
    <script>
        var content = new Vue({
            el: '#page',
            data: {
                total: 30,
                disease: '',
                realname: '',
                illness: '',
                type_desc: '',
                sex: '',
                age: '',
                service_id: ''
            },
            created: function () {

            },
            mounted() {
                var _this = this;
                getToken(_this)
                _this.service_id = GetQueryString('service_id');
                console.log(JSON.parse(localStorage.userItem))
                var item = JSON.parse(localStorage.userItem)
                _this.realname = item.doctor.realname
                _this.illness = item.doctor.illness
                _this.type_desc = item.translates.type_desc
                _this.sex = item.doctor.translates.sex
                _this.age = item.doctor.translates.age

                axios.get(doctorApi + '/service/' + _this.service_id)
                    .then(function (res) {
                        if (res.status == 200) {
                            console.log(res)
                            _this.disease = res.data.data.title
                        }
                    })
            },
            methods: {
                adviceBtn: function () {
                    location.href = "advice_task.html";
                }
            }
        })
    </script>
