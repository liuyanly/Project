.cc {
  font-size: 14px;
  color: #4CC6D8;
}
.header-title {
  line-height: 50px;
  border-bottom: 1px solid #eff1fa;
  padding: 0 20px;
  font-size: 14px;
  color: #333;
}
.Withdrawals-box {
  padding: 44px 14px;
}
.Withdrawals-box .input-box {
  margin-bottom: 20px;
}
.Withdrawals-box .input-box .title {
  font-size: 14px;
  color: #999;
  width: 85px;
  text-align: right;
  line-height: 30px;
}
.Withdrawals-box .input-box .date-select {
  margin-left: 16px;
  width: 236px;
  margin-right: 16px;
}
.Withdrawals-box .input-box select,
.Withdrawals-box .input-box input {
  width: 236px;
  padding: 0 10px;
  height: 30px;
  line-height: 30px;
  border: 1px solid #eff1fa;
  font-size: 14px;
  color: #333;
}
.Withdrawals-box .input-box input {
  width: 214px;
  padding: 0 10px;
  height: 28px;
  line-height: 28px;
  margin-left: 16px;
}
.Withdrawals-box .input-box .add-card {
  font-size: 14px;
  color: #4cc6d8;
  line-height: 30px;
  cursor: pointer;
  margin-left: 16px;
}
.Withdrawals-box .pay-btn {
  margin-left: 112px;
  width: 136px;
  height: 40px;
  background: #4cc6d8;
  border-radius: 3px ;
  line-height: 40px;
  text-align: center;
  border: none;
  font-size: 14px;
  color: #fff;
  margin-top: 10px;
  cursor: pointer;
}
.Withdrawals-box .select-box {
  width: 252px;
  height: 30px;
  cursor: pointer;
}
.Withdrawals-box .select-box .date-select {
  width: 214px;
  padding: 7px 10px;
  line-height: 15px;
  border: 1px solid #eff1fa;
  font-size: 14px;
  color: #333;
}
.Withdrawals-box .select-box .date-select img {
  margin-right: 6px;
  width: 16px;
  height: 16px;
}
.Withdrawals-box .select-box .c1 {
  color: #2748ab;
  margin-left: 5px;
}
.Withdrawals-box .select-box .nothing {
  color: #999;
}
.Withdrawals-box .select-box .xiaLa {
  right: 7px;
  top: 12px;
}
.Withdrawals-box .select-box .select-ul {
  background: #fff;
  top: 33px;
  left: 16px;
  border: 1px solid #ddd;
  border-bottom-left-radius: 10px;
  border-bottom-right-radius: 10px;
  border-top: none;
}
.Withdrawals-box .select-box .select-ul li {
  line-height: 39px;
  height: 39px;
  width: 214px;
  padding: 0 10px;
  font-size: 14px;
  color: #333;
}
.Withdrawals-box .select-box .select-ul li:last-child {
  border-bottom: none;
}
.Withdrawals-box .select-box .select-ul li img {
  margin-right: 6px;
  width: 16px;
  height: 16px;
  margin-top: 12px;
}
.Withdrawals-box .select-box .select-ul .current,
.Withdrawals-box .select-box .select-ul li:hover {
  background: #4CC6D8;
  color: #fff;
}
