/*---reset start ---*/
body, h1, h2, h3, h4, h5, h6, hr, p, blockquote, pre,
dl, dt, dd, ul, ol, li, th, td, div, span, img,
fieldset, lengend, button, input, select, textarea{
  margin: 0;
  padding: 0;
}
ul,
li {
  list-style: none;
}
*:focus {
  outline: none;
}
/* img border */
fieldset,
img {
  border: 0;
}
img {
  vertical-align: middle;
}
/* table */
table {
  /*border-collapse: collapse;border-spacing: 0;*/
  font: inherit;
}
th {
  text-align: left;
}
input,
textarea,
select {
  font-size: 100%;
}
input,
textarea,
select {
  *font-size: 100%;
}
legend {
  color: #000;
}
/* html5 */
article,aside,dialog,footer,header,section,footer,nav,figure,menu{ display:block }
* {
  -webkit-box-sizing: border-box;
     -moz-box-sizing: border-box;
          box-sizing: border-box;
}
*:before,
*:after {
  -webkit-box-sizing: border-box;
     -moz-box-sizing: border-box;
          box-sizing: border-box;
}
body,
button,
input,
select,
textarea {
  /* for ie */
  font: 12px/1.5 'Hiragino Sans GB W3', 'STHeiti', '微软雅黑', '黑体';
}

body {
  font-family: 'Hiragino Sans GB W3', 'STHeiti', '微软雅黑', '黑体';
  font-size: 14px;
  line-height: 1.5;
  color: #333333;
  background: #ffffff;
}
a {
  color: #333333;
  text-decoration: none;
}
a:hover,
a:focus {
  color: #333333;
  text-decoration: none;
}
a:focus {
  outline: none;
}
.clearfix:after {
  content: ".";
  display: block;
  height: 0;
  font-size: 0;
  clear: both;
  visibility: hidden;
}
.clearfix {
  display: inline-table;
}
* html.clearfix {
  *zoom: 1;
}
*:first-child + html .clearfix {
  zoom: 1;
}
/* Hides from IE-mac */
* html .clearfix {
  height: 1%;
}
.clearfix {
  display: block;
}
/* End hide from IE-mac */
* + html .clearfix {
  min-height: 1%;
}
/*---reset end ---*/
/*--- common ---*/
.fl {
  float: left;
}
.fr {
  float: right;
}
.pr {
  position: relative;
}