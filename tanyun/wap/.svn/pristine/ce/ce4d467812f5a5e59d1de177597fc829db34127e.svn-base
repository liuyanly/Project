html,body{
    height: 100%;
    font-family: '微软雅黑', 'STHeitiSC-Light', 'Helvetica-Light', arial, sans-serif;
    background: #ffffff;
    color: #999999;
    font-size: 0.7rem; /*14*2/40*/
}
img {
    max-width: 100%;
    vertical-align: top;
}
.clearfix{
    display: inline-block;
}
.clearfix:after{
    display: block;
    content: ".";
    height: 0;
    line-height: 0;
    clear: both;
    visibility: hidden
}
.show{
    display:block !important;
}
.hide{
    display:none !important;
}
 /*字体大小*/
.h4 {
    font-size: 1rem; /*20*2/40*/
}
small {
    font-size: 0.6rem; /*12*2/40*/
}
/*按钮*/
.btn {
    display: inline-block;
    padding: 0.2rem 0.8rem; /*4*2/40 16*2/40*/
    vertical-align: middle;
    text-align: center;
    font-size: 0.7rem; /*14*2/40*/
    cursor: pointer;
    border: 1px solid transparent;
    border-radius: 2px;
}
.btn-primary {
    background: #2EA2FD;
    border-color: #2EA2FD;
    color: #fff;
}
.btn-default {
    background: #fff;
    border-color: #666;
    color: #333;
}
.btn-warning {
    background: #FCB14F;
    border-color: #FCB14F;
    color: #fff;
}
.btn-disabled {
    background: #999;
    border-color: #999;
    color: #fff;
}
.flex-item{
    display: -webkit-flex;  /* 新版本语法: Chrome 21+ */
    display: flex;          /* 新版本语法: Opera 12.1, Firefox 22+ */
    display: -webkit-box;   /* 老版本语法: Safari, iOS, Android browser, older WebKit browsers. */
}
.flex-item .flex{
  -webkit-flex: 1;        /* Chrome */
  flex: 1;                /* NEW, Spec - Opera 12.1, Firefox 20+ */
  -webkit-box-flex: 1;     /* OLD - iOS 6-, Safari 3.1-6 */
}
/* 健康数据 */
.container {
   width: 100%;
   height: 100%;
}
.nav {
    height: 2rem; /*40*2/40*/
    background-color: #fff;
}
.nav ul {
 height: 2rem; /*40*2/40*/
 border-bottom: #eeeeee 1px solid;
}
.nav li {
  height:1.9rem; /*38*2/40*/
  line-height: 1.9rem; /*38*2/40*/
  color: #999999;
  font-size: 0.8rem; /*16*2/40*/
  text-align: center;
}
.nav li.active {
  font-weight: bold;
  color: #607D8B;
  border-bottom: #607D8B 2px solid;
}
.energy {
    padding: 0.5rem 0.75rem; /*10*2/40   15*2/40*/
}
.energy .title {
  height: 1.5rem; /*30*2/40*/
}
.energy .title span{
  display: block;
  height: 1.5rem; /*30*2/40*/
  background: #607D8B;
  line-height: 1.5rem; /*30*2/40*/
  color: #ffffff;
  font-size: 0.7rem;/*14*2/40*/
  text-align: center;
}
.energy .title span:first-child{
    border-radius: 6px 0 0 0;
}
.energy .title span:last-child{
    border-radius: 0 6px 0 0;
}
.energy .title span.active{
    background: #84CCCA;
}
.energy .conbox {
    height: 6rem; /*120*2/40*/
    height: 9rem;
    padding: 0.5rem; /*10*2/40*/
    background: rgba(96,125,139,0.10);
    border-radius: 0 0 6px 6px;
}
.energy .day {
    line-height: 6rem;/*120*2/40*/
    line-height: 9rem;
    font-size: 0.8rem;/*16*2/40*/
    text-align: center;
    color: #607D8B;
}
.energy .day span{
    font-size: 1.8rem;/*36*2/40*/
}
