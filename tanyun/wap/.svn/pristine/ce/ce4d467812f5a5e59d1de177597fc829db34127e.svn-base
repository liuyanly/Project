/* 弹出框 */
.modal {
    position: fixed;
    top: 0;
    right: 0;
    bottom: 0;
    left: 0;
    z-index: 1050;
    display: none;
    overflow: hidden;
    -webkit-overflow-scrolling: touch;
    outline: 0;
    background: rgba(0,0,0,0.40);
}
.modalBox {
    position: absolute;
    top: 6rem;
    left:1rem;
    right: 1rem;
    bottom: 3rem;
    background: rgba(252,252,252,.95);
    border-radius: 12px;
}
.modalTitle {
    height: 2rem;
    line-height: 2rem;
    font-size:0.8rem;
    background: #84CCCA;
    text-align: center;
    color: #fff;
    border-radius: 12px 12px 0 0;
}
.modalCon {
    position: absolute;
    top:2rem;
    bottom:0;
    width: 100%;
    overflow: auto;
    -webkit-overflow-scrolling:touch;
}
.close {
    position: absolute;
    left: 50%;
    bottom: 1rem;
    display: inline-block;
    width: 1.5rem;
    height: 1.5rem;
    margin-left: -0.75rem;
    background-image: url('../images/cancel@2x.png');
    background-repeat: no-repeat;
    background-size: 1.5rem 1.5rem; /*7*2/40 12*2/40*/
    cursor: pointer;
}
@media (-webkit-min-device-pixel-ratio: 3),(min-device-pixel-ratio: 3) {
    .close{
        background-image: url('../images/cancel@2x.png');
    }
}
